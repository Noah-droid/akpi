{
  "routes": [
    {
      "name": "auth",
      "path": "/auth",
      "target": "http://localhost:8000",
      "auth": false,
      "cache": "5 minutes"
    },
    {
      "name": "merchant",
      "path": "/merchant",
      "target": "http://localhost:8000",
      "auth": false,
      "rateLimit": {
        "windowMs": 60000,
        "max": 100
      },
      "cache": "5 minutes"
    },
    {
      "name": "docs",
      "path": "/docs",
      "target": "http://localhost:8000",
      "auth": false,
      "cache": "10 minutes"
    },
    {
      "name": "websocket",
      "path": "/api",
      "target": "ws://localhost:8080",
      "auth": false,
      "websocket": true
    }
  ]
}